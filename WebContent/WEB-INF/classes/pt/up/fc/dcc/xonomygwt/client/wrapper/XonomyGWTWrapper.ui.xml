	xmlns:w="urn:import:pt.up.fc.dcc.xonomygwt.client.wrapper">

	<ui:with field="resources" type="pt.up.fc.dcc.xonomygwt.client.resources.Resources" />
	
	<ui:style
		type="pt.up.fc.dcc.xonomygwt.client.wrapper.XonomyGWTWrapper.BaseStyle">
		
		.hide {
