<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:w='urn:import:pt.up.fc.dcc.mooshak.client.widgets'
	xmlns:c="urn:import:com.google.gwt.user.cellview.client" 
	xmlns:u="urn:import:pt.up.fc.dcc.mooshak.client.utils">
    
    <ui:with field='i18n' 
    	type='pt.up.fc.dcc.mooshak.client.guis.judge.i18n.JudgeConstants' />

	<ui:style src="Listing.css" />

	<g:HTMLPanel styleName='{style.base}'>
		
		<div>
			<g:HTMLPanel ui:field="filters">
			
			</g:HTMLPanel>
		</div>
		<!-- <g:Label styleName='{style.header}' ui:field='header'/> -->
		<c:DataGrid styleName='{style.grid}' ui:field='grid'/>
		<c:SimplePager styleName='{style.pager}' ui:field='pager'/>
		
		<!-- XML loaded Detail -->
		<w:CardPanel styleName="{style.detail}" ui:field="detail"/>
		<!-- <g:HTMLPanel styleName="{style.detail}" ui:field="detail"> 
			
		</g:HTMLPanel>-->
	</g:HTMLPanel>


</ui:UiBinder>