<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:pt.up.fc.dcc.mooshak.client.widgets"
	xmlns:u="urn:import:pt.up.fc.dcc.mooshak.client.utils">

	<ui:style src="../Dialogs.css" />
	
	<g:HTMLPanel styleName="{style.runTournament}">
		<g:HTMLPanel styleName="{style.container}">
			<g:Label styleName="{style.lbl-title}">Select Players</g:Label>
			<g:Label styleName="{style.lbl-desc}">Only players with at least 1 accepted
				submission are listed</g:Label>
			<g:ListBox multipleSelect="true" ui:field="lbPlayersSelector"
				width="100%" />
		</g:HTMLPanel>

	</g:HTMLPanel>

</ui:UiBinder>
