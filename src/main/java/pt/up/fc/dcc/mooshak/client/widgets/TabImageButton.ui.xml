	<ui:style src="TabButton.css"  
	   type ="pt.up.fc.dcc.mooshak.client.widgets.TabImageButton.BaseStyle"/>

	<ui:with field="images" 
		type="pt.up.fc.dcc.mooshak.client.images.ImageResources"/>
	
    <g:SimplePanel ui:field="pane">
        <m:CustomImageButton ui:field="image" outOpacity="1" overOpacity="1"
        	clickOpacity="1"/>
    </g:SimplePanel>
</ui:UiBinder> 