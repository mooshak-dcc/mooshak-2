<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	
	<ui:style src="TimeBar.css"/>
	
    <g:HTMLPanel>
    	<g:Label styleName='{style.timeLabel}' ui:field="startLabel"></g:Label>
    	<g:SimplePanel styleName='{style.duration}' ui:field="duration">
	        <g:SimplePanel styleName='{style.elapsed}' ui:field="elapsed"/>
	    </g:SimplePanel>
        <g:Label styleName='{style.timeLabel}' ui:field="endLabel"></g:Label>
    </g:HTMLPanel>
</ui:UiBinder> 